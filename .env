# Включаем source maps для отладки
GENERATE_SOURCEMAP=true

# Отключаем автоматическое открытие браузера
BROWSER=none

# Порт для dev сервера
PORT=3000

# Включаем подробные source maps для отладки
REACT_APP_GENERATE_SOURCEMAP=true

# Отключаем минификацию в development
DISABLE_ESLINT_PLUGIN=true

SKIP_PREFLIGHT_CHECK=true
FAST_REFRESH=true
CI=false
